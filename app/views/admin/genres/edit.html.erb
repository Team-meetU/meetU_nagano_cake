<%= render 'admin/homes/error_form', js: @genre %>
<%#div class="container takasa"%>
  <div class="row">

    <div class="col-xs-9">
      <h2>ジャンル編集</h2><br>
      <%= form_for @genre, url: admin_genre_path(@genre.id) do |f| %>
    <table class = "table">
        <div class="field row">
          <tr><td>
          <lavel>ジャンル名</lavel>
          </td><td>
          <%= f.text_field :name %>
          </td><td>
          <%= f.submit "変更を保存", class: "btn btn-primary" %>
          </td></tr>
         </div>
      <% end %>
  </div>
</div>